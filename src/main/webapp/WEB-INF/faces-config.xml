<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">

    <application>
        <resource-handler>org.omnifaces.resourcehandler.CombinedResourceHandler</resource-handler>
        <message-bundle>messages</message-bundle>
        <resource-bundle>
            <base-name>messages</base-name>
            <var>msg</var>
        </resource-bundle>
        
         <converter>
            <converter-id>guarantorConverter</converter-id>
            <converter-class>com.controller.GuarantorConverter</converter-class>
        </converter>
        
          <converter>
            <converter-id>requestConverter</converter-id>
            <converter-class>com.controller.RequestConverter</converter-class>
        </converter>
        
        <converter>
            <converter-id>categoryConverter</converter-id>
            <converter-class>com.controller.CategoryConverter</converter-class>
        </converter>
        
         <converter>
            <converter-id>testConverter</converter-id>
            <converter-class>com.controller.TestConverter</converter-class>
        </converter>
         <converter>
            <converter-id>doctorConverter</converter-id>
            <converter-class>com.controller.DoctorConverter</converter-class>
        </converter>
    </application>

</faces-config>
